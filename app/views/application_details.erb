<h1>Application Details</h1>
<h2>Source: <%= @source.identifier %></h2>

<h2>Most Requested URLs</h2>

<% @url_counts.each do |url, count| %>
<p>URL:<%= url %> Count: <%= count %></p>
<% end  %>

<h2>Browser Breakdown</h2>
<% @browser_counts.each do |browser, count| %>
<p>Browser: <%= browser %> Count: <%= count %></p>
<% end %>

<h2>OS Breakdown</h2>
<% @os_counts.each do |os, count| %>
<p>Operating System: <%= os %> Count: <%= count %></p>
<% end %>

<h2>Screen Resolution Across Requests</h2>
<% @resolutions.each do |height, width| %>
<p>Resolution Height: <%= height %> Resolution Width: <%= width %></p>
<% end %>

<h2>Average Response Time Per URL</h2>
<% @response_times.each do |url, response_time| %>
<p>URL: <%= url %> Response Time: <%= response_time %></p>
<% end %>

<h2>Source URL Index</h2>
<% @url_counts.each do |url| %>
<p><a href= "<%= %>">URL: <%= url %></a></p>
<% end %>=
